pipeline {
	agent any

	parameters{
		string(name: 'Greeting',defaultValue: 'hello',description: 'How should I greet the world?')

	 string(name: 'Project_Path', defaultValue: 'WebApplication1.sln')

	}

	stages{	 
	
		stage('Example'){
			steps{
				echo "${params.Greeting} World!"


			}
		}
		stage('build'){

			steps{
						  bat 'dotnet build %Project_Path% -p:Configuration=release -v:q'

			}
		}
		
		
	}
}
