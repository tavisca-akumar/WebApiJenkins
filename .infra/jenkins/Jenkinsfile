pipeline {
	agent any

	parameters{
		string(name: 'Greeting',defaultValue: 'hello',description: 'How should I greet the world?')

		 choice(name: 'CHOICE', choices: ['One', 'Two', 'Three'], description: 'Pick something')


	}

	stages{	 
	
		stage('Example'){
			steps{
				echo "${params.Greeting} World!"
			}
		}
		
		
	}
}
